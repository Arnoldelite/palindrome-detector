{
  "name": "palindrome-detector",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "jest": {
    "testEnvironment": "jsdom",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/app/src/**/*.{js}",
      "<rootDir>/server/src/**/*.{js}",
      "<rootDir>/server/index.js"
    ],
    "globalSetup": "<rootDir>/config/test-setup.js",
    "globalTeardown": "<rootDir>/config/test-teardown.js",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(css|less|scss)$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.(bmp|gif|jpg|jpeg|png|psd|svg|webp)$": "<rootDir>/config/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "scripts": {
    "mongodb": "docker-compose -f ./dev.yml kill && docker-compose -f ./dev.yml up -d --build --remove-orphans",
    "aws-staging": "chmod +x ./scripts/aws-staging.sh && ./scripts/aws-staging.sh",
    "build": "docker-compose kill && docker-compose up -d --build --remove-orphans",
    "deploy": "chmod +x ./deploy.sh && ./deploy.sh",
    "open-browser": "npx opn http://localhost:3000/app",
    "apidoc": "npx apidoc -i server/src/ -o server/apidoc/",
    "test": "npx jest --no-cache --coverage --detectOpenHandles",
    "test-watch": "npx jest --no-cache --detectOpenHandles --watchAll",
    "build-app": "rm -rf server/app && npx parcel build app/index.html -d build --no-source-maps --cache-dir .cache/app --out-dir server/app --public-url /app/",
    "start-app": "npx parcel watch app/index.html --hmr-port 1234 --cache-dir .cache/app --out-dir server/app --public-url /app/",
    "build-server": "npx babel-node server/server.js",
    "start-server": "npx nodemon --inspect --exec babel-node server/server.js",
    "dev": "run-p  start-server start-app",
    "start": "run-s mongodb dev"
  },
  "dependencies": {
    "@antv/data-set": "^0.10.2",
    "antd": "^3.11.6",
    "antd-mobile": "^2.2.8",
    "axios": "^0.18.0",
    "bizcharts": "^3.5.10",
    "body-parser": "^1.19.1",
    "cors": "^2.8.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "mobx": "^5.6.0",
    "mobx-react": "^5.4.2",
    "mobx-react-router": "^4.0.5",
    "mobx-remotedev": "^0.3.6",
    "mongoose": "^5.13.13",
    "morgan": "^1.9.1",
    "parse": "^2.1.0",
    "parse-dashboard": "^1.2.0",
    "parse-server": "^3.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^16.6.3",
    "react-icons": "^4.3.1",
    "react-infinite-scroller": "^1.2.4",
    "react-particles-js": "^2.4.2",
    "react-router-dom": "^4.3.1",
    "reqwest": "^2.0.5",
    "schm": "^0.4.1",
    "serve-static": "^1.13.2",
    "shelljs": "^0.8.2",
    "toastr": "^2.1.4",
    "twit": "^2.2.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@types/morgan": "^1.9.3",
    "@types/parse": "^2.18.13",
    "apidoc": "^0.17.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.11.0",
    "cypress": "^8.7.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "opn-cli": "^4.0.0",
    "parcel-bundler": "^1.10.3",
    "regenerator-runtime": "^0.13.1",
    "sass": "^1.45.0",
    "supertest": "^3.3.0"
  }
}
